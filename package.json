{
  "name": "insta-slackbot",
  "version": "1.0.0",
  "description": "Experimenting on Instagram API and its integration with Slack",
  "main": "index.js",
  "scripts": {
    "test": "jest && npm run eslint",
    "start": "pm2 start ecosystem.config.js",
    "restart": "pm2 restart insta-slackbot",
    "stop": "pm2 stop insta-slackbot --watch",
    "delete": "pm2 delete insta-slackbot",
    "log": "pm2 logs insta-slackbot",
    "show": "pm2 show insta-slackbot",
    "eslint": "./node_modules/.bin/eslint --fix ./libs/*.js ./app.js ./bin/www ./routes/*.js",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
  "jest": {
    "roots": [
      "resources/tests"
    ]
  },
  "precommit": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imballinst/insta-slackbot.git"
  },
  "keywords": [
    "instagram",
    "slack",
    "api"
  ],
  "author": "Try Ajitiono",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Imballinst/insta-slackbot/issues"
  },
  "homepage": "https://github.com/Imballinst/insta-slackbot#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "botkit": "^0.5.4",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.27",
    "querystring": "^0.2.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "generate-changelog": "^1.1.0",
    "jest": "^19.0.2",
    "pre-commit": "^1.2.2"
  }
}
